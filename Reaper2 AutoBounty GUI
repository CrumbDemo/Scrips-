-- Scripts:

local function ILAFINZ_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local GUI = script.Parent
	local Acc1 = GUI.Acc1
	local Acc2 = GUI.Acc2
	local Acc1Tab = GUI.Acc1Tab
	local Acc2Tab = GUI.Acc2Tab
	local Colour = Color3.fromRGB(52,52,52)
	local Black = Color3.fromRGB(0,0,0)
	Acc1Tab.TextButton.MouseButton1Click:Connect(function()
		Acc2Tab.TextButton.TextColor3 = Colour
		Acc1Tab.TextButton.TextColor3 = Black
		Acc1.Visible = true
		Acc2.Visible = false
	end)
	Acc2Tab.TextButton.MouseButton1Click:Connect(function()
		Acc1Tab.TextButton.TextColor3 = Colour
		Acc2Tab.TextButton.TextColor3 = Black
		Acc2.Visible = true
		Acc1.Visible = false
	end)
end
coroutine.wrap(ILAFINZ_fake_script)()
local function RJHKE_fake_script() -- Reaper2GUIBounty.LocalScript 
	local script = Instance.new('LocalScript', Reaper2GUIBounty)

	local GUI = script.Parent.Frame
	local Shortcut = script.Parent.Button
	
	Shortcut.TextButton.MouseButton1Click:Connect(function()
		GUI.Visible = true
		Shortcut.Visible = false
	end)
	
	GUI.Header.Exit.TextButton.MouseButton1Click:Connect(function()
		GUI.Visible = false
		Shortcut.Visible = true
	end)
end
coroutine.wrap(RJHKE_fake_script)()

local Toggle1 = false
local Toggle2 = false

game.Players.PlayerRemoving:Connect(function(Player)
	if Player.Name == Name_2.Text then 
		Toggle_2.TextColor3 = Red
		Toggle_2.Text = 'Enable'
		_G.On2 = false
		wait(1)
		game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
	end
	if Player.Name == Name.Text then 
		Toggle.TextColor3 = Red
		Toggle.Text = 'Enable'
		_G.On = false
	end
end)

_G.On2 = false

Toggle.MouseButton1Click:Connect(function()
    getgenv().Target = Name.Text
    if _G.On2 == true then
        Toggle.TextColor3 = Red
        Toggle.Text = 'Enable'
        _G.On2 = false
	wait(1)
	game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
    else
        Toggle.TextColor3 = Green
        Toggle.Text = 'Disable'
        _G.On2 = true
    end
    spawn(function()loadstring(game:HttpGet(("https://raw.githubusercontent.com/CrimsonicDemon/Scrips-/main/Reaper2%20AutoBounty%20Acc1"),true))();end)
end)


_G.On = false
_G.FirstDone = false

Toggle_2.MouseButton1Click:Connect(function()
    getgenv().Target2 = Name_2.Text
    if _G.On == true then
    	_G.On = false
        Toggle_2.TextColor3 = Red
        Toggle_2.Text = 'Enable'
	if not game.Players[getgenv().Target2].Character then 
            game.Players[getgenv().Target2].CharacterAdded:Wait()
        end
     	game.Players[getgenv().Target2].Character:WaitForChild("HumanoidRootPart").Anchored = false
    else
    	_G.On = true
        Toggle_2.TextColor3 = Green
	game.Players.LocalPlayer.Character:WaitForChild("Head"):Destroy()
        Toggle_2.Text = 'Disable'
    end
    if _G.FirstDone == false then
	spawn(function()loadstring(game:HttpGet(("https://raw.githubusercontent.com/CrimsonicDemon/Scrips-/main/Reaper2%20AutoBounty%20Acc2"),true))();end)
    	_G.FirstDone = true
    end
    if not game.Players:FindFirstChild(getgenv().Target2) then
    	_G.On = false
    	Toggle_2.TextColor3 = Red
        Toggle_2.Text = 'Enable'
    end
    wait(1)
end)
